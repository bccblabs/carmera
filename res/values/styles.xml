<resources>
    <style name="CarmeraTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="android:textColor">@color/card_pressed</item>
        <item name="android:textColorPrimary">@color/card_pressed</item>
        <item name="preferenceTheme">@style/CarmeraTheme.PreferenceStyle</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="CarmeraTheme.PreferenceStyle" parent="@style/PreferenceThemeOverlay">
        <item name="android:textColorPrimary">@color/selected_item_color</item>
        <item name="android:textColorSecondary">@color/selected_item_color</item>
        <item name="android:textColorTertiary">@color/selected_item_color</item>
    </style>
    <style name="CarmeraTheme.ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:titleTextStyle">@style/CarmeraTheme.ActionBar.TitleTextStyle</item>
    </style>
    <style name="CarmeraTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/background_light</item>
    </style>
    <style name="DescStyle">
        <item name="android:layout_column">0</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/holo_blue_light</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="EntryStyle" parent="DescStyle">
        <item name="android:layout_column">1</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textColor">@android:color/background_light</item>
    </style>
    <style name="SmallDescStyle" parent="DescStyle">
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="SmallEntryStyle" parent="EntryStyle">
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="TableStyle">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TableRowStyle">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="TwoButtonLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">28dp</item>
        <item name="android:layout_marginRight">28dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>
    <style name="ButtonLeft">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="ButtonRight" parent="ButtonLeft">
    </style>
    <style name="EditTextStyle">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">28dp</item>
        <item name="android:layout_marginRight">28dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="price_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/background_light</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_margin">3dp</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>
    <style name="mileage_title" parent="price_title">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textSize">17sp</item>
    </style>
    <style name="dealer_info_title" parent="mileage_title">
    </style>
    <style name="feature_title" parent="mileage_title">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="listing_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/background_floating_material_light</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">top|right</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="listing_title_blue" parent="listing_title">
        <item name="android:textColor">@color/default_blue</item>
    </style>
    <style name="listing_title_small" parent="listing_title">
        <item name="android:layout_gravity">top|start</item>
        <item name="android:layout_margin">5dp</item>
    </style>
    <style name="entry_title" parent="listing_title">
        <item name="android:layout_gravity">top|start</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">5dp</item>
    </style>
    <style name="entry_title_right" parent="listing_title">
        <item name="android:layout_gravity">top|end</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_margin">5dp</item>
    </style>

    <style name="horizontal_bar_style">
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_height">300dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="single_horizontal_bar_style">
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_height">250dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="section_title" parent="listing_title">
        <item name="android:textSize">25sp</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>
    <style name="dealer_text" parent="mileage_title">
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="dealer_info_btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="dealer_info_container">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
    </style>
    <style name="circular_image_view">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
    </style>
    <style name="LinearLayout" />

    <style name="LinearLayout.GuillotineItem">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">@dimen/guillotine_small_margin</item>
    </style>

    <style name="TextView" />
    <style name="TextView.ToolbarTitle">
        <item name="android:gravity">center</item>
        <item name="android:textSize">24sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextView.GuillotineItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/guillotine_item_text_size</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/selected_item_color</item>
        <item name="android:layout_marginLeft">@dimen/guillotine_small_margin</item>
    </style>
    <style name="TextView.GuillotineItem.Selected">
        <item name="android:textColor">@color/selected_item_color</item>
    </style>

    <style name="Toolbar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/background_light</item>
    </style>
    <style name="ImageView" />
    <style name="ImageView.ContentItem">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/guillotine_small_margin</item>
    </style>
    <style name="ImageView.Hamburger" parent="Widget.AppCompat.ActionButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/abc_action_bar_overflow_padding_start_material</item>
    </style>
    <style name="ImageView.GuillotineItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/guillotine_medium_margin</item>
    </style>
    <style name="TagGroup.LargeBlue" parent="TagGroup">
        <item name="atg_borderColor">@color/default_blue</item>
        <item name="atg_textColor">#FFFFFF</item>
        <item name="atg_backgroundColor">@color/default_blue</item>
        <item name="atg_inputHintColor">@color/default_blue</item>
        <item name="atg_inputTextColor">@color/default_blue</item>
        <item name="atg_checkedBorderColor">@color/default_blue</item>
        <item name="atg_checkedTextColor">@color/default_blue</item>
        <item name="atg_checkedMarkerColor">@color/default_blue</item>
        <item name="atg_checkedBackgroundColor">#FFFFFF</item>
        <item name="atg_pressedBackgroundColor">@color/default_blue</item>
        <item name="atg_borderStrokeWidth">0.7dp</item>
        <item name="atg_textSize">15sp</item>
        <item name="atg_horizontalSpacing">9dp</item>
        <item name="atg_verticalSpacing">5dp</item>
        <item name="atg_horizontalPadding">14dp</item>
        <item name="atg_verticalPadding">4dp</item>
    </style>
    <style name="card.content_outer_layout.staggered">
        <item name="android:minHeight">20dp</item>
    </style>

    <style name="cardwithlist">
        <item name="android:layout_marginTop">@dimen/card_base_cardwithlist_list_margin_top</item>
        <item name="android:layout_marginLeft">@dimen/card_base_cardwithlist_list_margin_left</item>
        <item name="android:divider">@color/card_native_base_cardwithlist_divider_color</item>
        <item name="android:showDividers">middle</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/default_blue</item>
    </style>

    <style name="TagGroup.CarTags" parent="TagGroup.Small">
        <item name="atg_textSize">13sp</item>
        <item name="atg_borderStrokeWidth">0.8dp</item>
        <item name="atg_horizontalSpacing">8dp</item>
        <item name="atg_verticalSpacing">3dp</item>
        <item name="atg_horizontalPadding">12dp</item>
        <item name="atg_verticalPadding">3dp</item>
        <item name="atg_textColor">#49C120</item>
        <item name="atg_borderColor">#49C120</item>

    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/ic_stars_white_24dp</item>
        <item name="android:textColor">@color/card_pressed</item>
    </style>


    <style name="search_card_view">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>
    <style name="search_image_view">
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_width">@dimen/ed_img_lg_w</item>
        <item name="android:layout_height">@dimen/ed_img_lg_h</item>
    </style>
    <style name="search_container">
        <item name="android:background">@color/black_20</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="search_title" parent="price_title">
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="search_subtitle" parent="search_title">
        <item name="android:textSize">12sp</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="add_to_search_btn" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:background">@android:color/background_light</item>
    </style>

</resources>
